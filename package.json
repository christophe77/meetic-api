	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/esm/index.d.ts",
	"scripts": {
		"start": "yarn build && node ./dist/cjs/index.js",
		"build": "yarn build:esm && yarn build:cjs",
